<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <listener>
        <listener-class>com.sun.xml.ws.transport.http.servlet.WSServletContextListener</listener-class>
    </listener>
    <servlet>
        <servlet-name>BuscadorServlet</servlet-name>
        <servlet-class>py.una.pol.par.controllers.BuscadorServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogueoServlet</servlet-name>
        <servlet-class>py.una.pol.par.controllers.LogueoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ConfirmacionServlet</servlet-name>
        <servlet-class>py.una.pol.par.controllers.ConfirmacionServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>s</servlet-name>
        <servlet-class>py.una.pol.par.controllers.s</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CategoriaServlet</servlet-name>
        <servlet-class>py.una.pol.par.controllers.CategoriaServlet1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ParOnlineWS</servlet-name>
        <servlet-class>com.sun.xml.ws.transport.http.servlet.WSServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>AgregarCarrito</servlet-name>
        <servlet-class>py.una.pol.par.controllers.AgregarCarrito</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CategoriaServlets</servlet-name>
        <servlet-class>py.una.pol.par.controllers.CategoriaServlets</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UsuariosServlet</servlet-name>
        <servlet-class>py.una.pol.par.controllers.UsuariosServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>BuscadorServlet</servlet-name>
        <url-pattern>/BuscadorServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogueoServlet</servlet-name>
        <url-pattern>/LogueoServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ConfirmacionServlet</servlet-name>
        <url-pattern>/ConfirmacionServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>s</servlet-name>
        <url-pattern>/s</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CategoriaServlet</servlet-name>
        <url-pattern>/CategoriaServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ParOnlineWS</servlet-name>
        <url-pattern>/ParOnlineWS</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AgregarCarrito</servlet-name>
        <url-pattern>/AgregarCarrito</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CategoriaServlets</servlet-name>
        <url-pattern>/CategoriaServlets</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UsuariosServlet</servlet-name>
        <url-pattern>/UsuariosServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
